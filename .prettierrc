{
    "parser": "typescript",
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 4,
    "singleAttributePerLine": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-tailwindcss"],
    "bracketSameLine": false
}
